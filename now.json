{
  "version": 2,
  "name": "MCD CDP Portal",
  "alias": "mcd-cdp-portal-staging",
  "routes": [
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": "^/favicon.ico", "dest": "/favicon.ico" },
    { "src": "^/manifest.json", "dest": "/manifest.json" },
    { "src": "^/(.*)", "dest": "/index.html" }
  ],
 "github": {
    "autoAlias": false
  }
}
